generator client {
  provider        = "prisma-client"
  output   = "../app/generated/prisma"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CsvData {
  id        String   @id @default(cuid())
  name      String   // filename or dataset name
  data      Json     // store entire CSV as structured JSON
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
